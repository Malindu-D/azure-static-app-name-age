{
  "routes": [],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/styles.css", "/script.js", "/config.js"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' https://*.azurewebsites.net; style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-inline'",
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "X-XSS-Protection": "1; mode=block"
  },
  "platform": {
    "apiRuntime": "node:18"
  }
}
